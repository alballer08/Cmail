
{
  "manifest_version": 3,
  "name": "Cmail",
  "description": "Email Summarizer",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
  "default_popup": "index.html",
  "default_icon": "name.png"
}
}
